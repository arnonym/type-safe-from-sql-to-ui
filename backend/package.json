    "migrate": "bun run src/database/migrator.ts",
    "generate:database-types": "kysely-codegen"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "@types/pg": "^8.15.5",
    "typescript": "^5.9.3",
    "kysely-codegen": "^0.19.0"
  },
  "dependencies": {
    "@orpc/client": "^1.10.1",
    "@orpc/openapi": "^1.10.1",
    "@orpc/server": "^1.10.1",
    "@orpc/zod": "^1.10.1",
    "kysely": "^0.28.8",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "kysely-codegen": {
    "dialect": "postgres",
    "url": "postgres://demo:demo@localhost:5436/demo",
